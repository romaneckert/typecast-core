        "express": "4.17.1",
        "helmet": "3.20.1",
        "jsonwebtoken": "8.5.1",
        "mysql": "2.17.1",
        "nodemailer": "6.3.0",
        "pug": "2.0.4",
        "querystring": "0.2.0",
        "reflect-metadata": "0.1.13",
        "smtp-server": "3.5.0",
        "typeorm": "0.2.18"
    }
